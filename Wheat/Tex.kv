<Tex>:
    id: tex

    FloatLayout:
        pos_hint: {'x' : 0.0, 'y' : 0.0}
        size_hint: 1, 1
        canvas.before:
            Color:
                rgba: 1,1,1,1
            Rectangle:
                pos: self.pos
                size: self.size

        TextInput:
            pos_hint: {'x' : 0.1, 'y' : 0.05}
            size_hint: 0.8,0.2
