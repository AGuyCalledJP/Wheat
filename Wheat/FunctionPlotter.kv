#Backend
<FunctionPlotter>:
    id: functionplotter
    size_hint: 0.8,0.6

    FloatLayout:
        id: container
        pos_hint: {'x' : 0, 'y' : 0}
        size_hint: 1, 1
        canvas.before:
            Color:
                rgba: root.c1,root.c2,root.c3,0.5
            RoundedRectangle:
                pos: self.pos
                size: self.size

        FloatLayout:
            id: toggle_container
            pos_hint: {'x' : 0.4, 'y' : 0.925}
            size_hint: 0.1, 0.05
            canvas.before:
                Color:
                    rgba: root.c1,root.c2,root.c3,1
                RoundedRectangle:
                    pos: self.pos
                    size: self.size

            ToggleButton:
                state: "normal"
                id: hide_keyboard
                pos_hint: {'x': 0, 'y': 0}
                size_hint: .5, 1
                background_color: 0,0,0,0
                canvas.before:
                    Color:
                        rgba: 1,1,1,1
                    RoundedRectangle:
                        pos: self.pos
                        size: self.size
                text: "H"
                on_press:
                    hide_keyboard.pos_hint= {'x': 0.5, 'y': 0}
                    container.size_hint_x = input.hide_input()
                    display.size_hint_x = 0.945


                    functionplotter.size_hint_x = 0.4
                    check.size_hint = .1,.05
                    toggle_container.size_hint = 0.2,0.05
                    toggle_container.pos_hint = {'x': 0.775, 'y': 0.925}
                    hide_keyboard.text = "U"
                    equation.size_hint_x = 0.725
                    equation.pos_hint = {'x': 0.15, 'y': 0.05}
                on_release:
                    container.size_hint_x = input.hide_input(dohide=False)
                    hide_keyboard.pos_hint= {'x': 0, 'y': 0}
                    display.size_hint_x = 0.5

                    functionplotter.size_hint_x = 0.8
                    check.size_hint = .05,.05
                    toggle_container.size_hint = 0.1,0.05
                    toggle_container.pos_hint = {'x': 0.4, 'y': 0.925}
                    hide_keyboard.text = "H"
                    equation.size_hint_x = 0.375
                    equation.pos_hint = {'x': 0.085, 'y': 0.05}

        CheckBox:
            id: check
            pos_hint: {'x': 0.025,'y': 0.925}
            size_hint: .05,.05
            canvas.before:
                Color:
                    rgba: root.c1,root.c2,root.c3,0.5
                Rectangle:
                    pos: self.pos
                    size: self.size

        BoxLayout:
            id: display
            pos_hint: {'x' : 0.025, 'y': 0.025}
            size_hint: 0.475, 0.875
            canvas.before:
                Color:
                    rgba:1,1,1,1
                Rectangle:
                    pos: self.pos
                    size: self.size

        Equation:
            id: equation
            pos_hint: {'x': 0.085, 'y': 0.05}
            size_hint: 0.375,0.075
            canvas.before:
                Color:
                    rgba:.9,.9,.9,1
                RoundedRectangle:
                    pos: self.pos
                    size: self.size
            multiline: False
            text: root.equation_text

        Input:
            id: input
            pos_hint: {'x' : 0.5, 'y': 0.025}
            size_hint: 0.475, 0.875
            canvas.before:
                Color:
                    rgba: 1,1,1,1
                Rectangle:
                    pos: self.pos
                    size: self.size

            Keyboard:
                pos_hint: {'x' : 0.01, 'y': 0.01}
                size_hint: .98, .98
                id: key
                spacing: 2
                cols: 5

                Button:
                    background_color: 0,0,0,0
                    canvas.before:
                        Color:
                            rgba: root.c1,root.c2,root.c3,root.c4-0.1
                        RoundedRectangle:
                            pos: self.pos
                            size: self.size
                    font_size: root.fontSizer
                    text: u'\u221a'
                    on_press: functionplotter.ClickedSqrt()
                Button:
                    background_color: 0,0,0,0
                    canvas.before:
                        Color:
                            rgba: root.c1,root.c2,root.c3,root.c4
                        RoundedRectangle:
                            pos: self.pos
                            size: self.size
                    font_size: root.fontSizer
                    text: 'sin'
                    on_press: functionplotter.ClickedSin()
                Button:
                    background_color: 0,0,0,0
                    canvas.before:
                        Color:
                            rgba: root.c1,root.c2,root.c3,root.c4-0.1
                        RoundedRectangle:
                            pos: self.pos
                            size: self.size
                    font_size: root.fontSizer
                    text: 'cos'
                    on_press: functionplotter.ClickedCos()
                Button:
                    background_color: 0,0,0,0
                    canvas.before:
                        Color:
                            rgba: root.c1,root.c2,root.c3,root.c4
                        RoundedRectangle:
                            pos: self.pos
                            size: self.size
                    font_size: root.fontSizer
                    text: 'tan'
                    on_press: functionplotter.ClickedTan()
